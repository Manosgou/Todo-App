{
  "expo": {
    "name": "TodoApp",
    "slug": "TodoApp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/todo_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D95525"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.manosgou.todo"
    },
    "android": {
      "package": "com.manosgou.todo",
      "versionCode": 1
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}